;AAAA,aAAO,OAAO,CAAC,CAAC,CAAC,OAAH,CAAd;AAAA,KAHT;AAID,GAzBY;AA0Bb,EAAA,OAAO,EAAE;AACP,IAAA,aADO,yBACO,EADP,EACW;AAChB,WAAK,QAAL,GAAgB,KAAK,QAAL,CAAc,MAAd,CAAqB,UAAC,OAAD,EAAa;AAChD,eAAO,OAAO,CAAC,EAAR,KAAe,EAAtB;AACD,OAFe,CAAhB;AAGD,KALM;AAMP,IAAA,cANO,0BAMQ,EANR,EAMY;AACjB,UAAI,CAAA,GAAI,KAAK,QAAL,CAAc,IAAd,CAAmB,UAAC,OAAD,EAAa;AACtC,eAAO,OAAO,CAAC,EAAR,KAAe,EAAtB;AACD,OAFO,CAAR;AAGA,MAAA,CAAC,CAAC,QAAF,GAAa,CAAC,CAAC,CAAC,QAAhB;AACD;AAXM;AA1BI,CAAf","sourcesContent":["<template>\n  <div class=\"home\">\n    <FilterNav @filter=\"current = $event\" :current=\"current\"></FilterNav>\n    <div v-if=\"projects.length\">\n      <div v-for=\"prj in filterProject\" :key=\"prj.id\">\n        <SingleProject\n          :project=\"prj\"\n          @delete=\"deleteProject($event)\"\n          @edit=\"toggleComplete($event)\"\n        ></SingleProject>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport SingleProject from \"../components/SingleProject.vue\";\nimport FilterNav from \"../components/FilterNav.vue\";\n// @ is an alias to /src\nexport default {\n  name: \"Home\",\n  components: { SingleProject, FilterNav },\n  data() {\n    return {\n      projects: [],\n      current: 'all',\n    };\n  },\n  computed:{\n    filterProject(){\n      if(this.current == \"going\"){\n        return this.projects.filter(project=> !project.complete)\n      }else if(this.current == \"complete\" ){\n        return this.project.filter(project => project.complete)\n      }else{\n        return this.projects\n      }\n    }\n  },\n  mounted() {\n    fetch(\"http://localhost:3000/project\")\n      .then((res) => res.json())\n      .then((data) => (this.projects = data))\n      .catch((e) => console(e.message));\n  },\n  methods: {\n    deleteProject(id) {\n      this.projects = this.projects.filter((project) => {\n        return project.id !== id;\n      });\n    },\n    toggleComplete(id) {\n      let p = this.projects.find((project) => {\n        return project.id === id;\n      });\n      p.complete = !p.complete;\n    },\n  },\n};\n</script>\n"],"sourceRoot":""}]}