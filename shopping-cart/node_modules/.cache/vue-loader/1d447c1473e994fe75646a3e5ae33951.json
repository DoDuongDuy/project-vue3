C,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACrB,CAAC;AACL,CAAC","file":"/home/duy/Documents/vue/mini-project/shopping-cart/src/components/products/ProductSummaryCard.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"card\">\n    <h3>{{ product.name }}</h3>\n    <h5 class=\"price\">Price: {{ product.price }} $</h5>\n    <p class=\"description\">Description: {{description}} </p>\n    <p class=\"text-mute\">{{ product.category }}</p>\n    <button class=\"view-product-button\" @click=\"$emit('view-product', product)\">\n      View\n    </button>\n  </div>\n</template>\n\n<script>\nimport { computed } from \"@vue/runtime-core\";\nexport default {\n  props: [\"product\"],\n    setup(props) {\n        const description = computed(()=> props.product.description.substring(0,50) ); \n      return {description}\n    },\n};\n</script>\n\n<style lang=\"scss\">\n.card {\n  width: 80%;\n  margin: 10%;\n  padding: 10px;\n  border-radius: 5px;\n  background-color: white;\n  box-shadow: 0 0 5px gray;\n  h5.price {\n    color: gray;\n  }\n  p.description {\n    font-size: 0.85rem;\n  }\n  p.text-muted {\n    color: gray;\n  }\n}\nbutton.view-product-button {\n  padding: 10px;\n  background-color: green;\n  border: none;\n  color: white;\n  font-weight: bold;\n  font-size: 1.15rem;\n  border-radius: 5px;\n  cursor: pointer;\n}\n@media (min-width: 500px) {\n  .card {\n    width: 350px;\n    margin: 10px;\n  }\n}\n</style>\n"]}]}